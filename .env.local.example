# Obsidian to Astro Sync Configuration
# Copy this file to .env.local and update with your paths

# Absolute path to your Obsidian vault directory
# Example: /Users/goyo/Documents/MyVault
OBSIDIAN_PATH=/path/to/your/obsidian/vault

# Relative path from project root to blog post directory
# Default: src/content/blogPost
PROJECT_PATH=src/content/blogPost

# Relative path from project root to images directory
# Default: src/Images
IMAGE_PATH=src/Images
